{
  "name": "@groov/graphql",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "apollo-server": "^2.9.14",
    "apollo-server-express": "^2.9.14",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv-config": "^0.1.1",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "http": "^0.0.0",
    "ncp": "^2.0.0",
    "pg": "^7.15.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "5.21.5",
    "sequelize-typescript": "^1.1.0",
    "ts-node": "8.7.0",
    "type-graphql": "^0.17.6",
    "typeorm": "^0.2.24",
    "typescript": "3.8.3",
    "uuid": "7.0.2"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "build:ts": "yarn tsc --build tsconfig.json",
    "start:dev": "npm run build:dev",
    "build:dev": "nodemon 'src/index.ts' --exec 'ts-node' src/index.ts -e ts,graphql",
    "generate": "yarn graphql-codegen",
    "lint": "eslint 'src/**/*.ts?(x)' && npm run prettier:check",
    "prettier": "prettier --write 'src/**/*.ts?(x)'",
    "prettier:check": "prettier -c 'src/**/*.ts?(x)'"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/compression": "^1.0.1",
    "@types/express": "^4.17.2",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/node": "^13.1.1",
    "@types/sequelize": "^4.28.8",
    "@types/uuid": "7.0.2",
    "@types/validator": "^12.0.1",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^0.7.1",
    "graphql-import-node": "^0.0.4",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2"
  }
}
