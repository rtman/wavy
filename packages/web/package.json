{
  "name": "@groov/web",
  "version": "0.0.5",
  "private": true,
  "workspaces": {
    "nohoist": [
      "*react*",
      "*react*/**",
      "*@types/react*",
      "*@types/react*/**",
      "*react-scripts*",
      "*react-scripts*/**"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.3.4",
    "@date-io/moment": "1.3.9",
    "@material-ui/core": "^4.9.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "4.0.0-alpha.48",
    "@material-ui/pickers": "^3.2.10",
    "axios": "^0.19.2",
    "detect-browser": "5.2.0",
    "email-validator": "^2.0.4",
    "firebase": "^7.7.0",
    "graphql": "^14.5.8",
    "http-status-codes": "^1.4.0",
    "immutability-helper": "^3.0.1",
    "material-ui-nested-menu-item": "^1.0.2",
    "moment": "^2.29.0",
    "notistack": "^0.9.17",
    "patch-package": "^6.2.0",
    "polished": "^4.0.5",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-dropzone": "11.0.2",
    "react-firebase-hooks": "^2.1.1",
    "react-helmet": "^5.2.1",
    "react-hook-form": "6.11.2",
    "react-images-upload": "^1.2.8",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.0",
    "react-virtualized-auto-sizer": "1.0.2",
    "react-window": "1.8.6",
    "react-window-infinite-loader": "1.0.5",
    "smoothscroll-polyfill": "0.4.4",
    "styled-components": "^5.0.0",
    "use-context-selector": "1.1.4",
    "uuidv4": "^6.2.3"
  },
  "scripts": {
    "generate-types": "yarn graphql-codegen",
    "start": "serve -s build",
    "start:local": "react-scripts start",
    "build": "react-scripts build",
    "build:ts": "yarn tsc --build tsconfig.json",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "eslint": "eslint 'src/**/*.ts?(x)'",
    "eslintfix": "eslint 'src/**/*.ts?(x)' --fix",
    "postinstall": "patch-package",
    "lint": "eslint 'src/**/*.ts?(x)' && yarn prettier:check",
    "prettier": "prettier --write 'src/**/*.ts?(x)'",
    "prettier:check": "prettier -c 'src/**/*.ts?(x)'",
    "setBackendLoadBalancerIp": "node ./scripts/setBackendLoadBalancerIp.js",
    "deploy:gae": "yarn build && gcloud app deploy"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^24.9.0",
    "@types/react": "17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^5.0.15",
    "@types/react-router-dom": "5.1.7",
    "@types/react-virtualized-auto-sizer": "1.0.0",
    "@types/react-window": "1.8.2",
    "@types/react-window-infinite-loader": "1.0.3",
    "@types/smoothscroll-polyfill": "0.3.1",
    "@types/styled-components": "^5.1.0",
    "@welldone-software/why-did-you-render": "6.0.0-rc.1",
    "babel-plugin-styled-components": "^1.10.6",
    "eslint": "^7.17.0",
    "jest": "^24.9.0",
    "typescript": "4.1.2",
    "typescript-styled-plugin": "^0.15.0"
  }
}
